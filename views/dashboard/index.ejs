      <div class="main-header-name-and-isin">
          <span class="main-header-name-and-isin-name"> 
           <!--<%= unternehmen.company_data.id %> --> Apple Inc.
        </span>
        <span class="main-header-name-and-isin-isin">
          ISIN: US0378331005 
        </span>
      </div>
      
      <%- include ('_aktienKursGraph.ejs') %>

      <div class="main-header-graph">
      <div id="aktienKursGraph"></div>        
      </div>


      <div class="main-header-aktienkurs-box">
        <span class="main-header-aktienkurs-box-kurs" style="color:<%= calculated.calculated_data.colorForChange %>">
          <%= calculated.calculated_data.changePercent %> %
