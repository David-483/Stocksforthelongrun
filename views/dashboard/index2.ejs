            </button>

            <button id="aktienkurs_all" class="active">
              ALL
            </button>
          </div>
          <div id="aktienKursGraphTotal"></div>
        </div>
    </div>

    <i class="fa-solid fa-chevron-right"></i>
    <!-- Flickity HTML init -->
    <div class="carousel" data-flickity='{ "wrapAround": true, "autoPlay": false, "pauseAutoPlayOnHover": false }'>

      <div class="carousel-slider">

          <%- include ('_tableCarousel_income.ejs') %>

      </div>

   

    <div class="carousel-slider">

      <%- include ('_tableCarousel_balance.ejs') %>
 
  </div>
  
  <div class="carousel-slider">

    <%- include ('_tableCarousel_cashflow.ejs') %>

</div>
</div>


<div id="chart">
  <div class="toolbar">
  </div>
