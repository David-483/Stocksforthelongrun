var gurutrend_categories = "<%= Guru.trendChartData.sectorOverTimeCategories %>";

gurutrend_categories = gurutrend_categories.split(',');

  sectorOverTimeChart= {
          series: [{
          name: 'Energie',
          data: gurutrend_Energie
        }, {
          name: 'Finanzen',
          data: gurutrend_Finanzen
        }, {
          name: 'Fonds',
          data: gurutrend_Fonds
        }, {
          name: 'Gesundheit',
          data: gurutrend_Gesundheit
        }, {
          name: 'Basiskonsumgüter',
          data: gurutrend_Basiskonsumgüter
