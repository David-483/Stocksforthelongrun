          chart: {
          type: 'bar',
          height: 350,
          stacked: true,
          stackType: '100%'
        },
        responsive: [{
          breakpoint: 480,
          options: {
            legend: {
              position: 'bottom',
              offsetX: -10,
              offsetY: 0
            }
          }
        }],
        xaxis: {
          categories: gurutrend_categories,
        },
        fill: {
          opacity: 1
        },
        legend: {
          position: 'bottom',
          offsetX: 0,
          offsetY: 50
        },
