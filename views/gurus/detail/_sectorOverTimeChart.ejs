<script>
var gurutrend_Energie = [<%= Guru.trendChartData.Energie %>];
var gurutrend_Finanzen = [<%= Guru.trendChartData.Finanzen %>];
var gurutrend_Fonds = [<%= Guru.trendChartData.Fonds %>];
