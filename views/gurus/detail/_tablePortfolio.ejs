
<div class="carousel-slider">
    <div class="tableHeader">

        <table class="carousel-table" cellpadding="0" cellspacing="0" border="0">
            <thead>
                <tr>
                    <th>Logo</th>
                    <th>Name</th>
                    <th>Anteil im Portfolio</th>
                    <th>Shares</th>
                    <th>Shares Value Veränderung ggü Vorjahr</th>
                    <th>value</th>
                    <th>value pro Share</th>
                </tr>
            </thead>
        </table>
    
    </div>
    
    <div class="tableInhalt">
    
        <table class="carousel-table" cellpadding="0" cellspacing="0" border="0">
    
            <tbody>
                <% for(var i=0; i < 5; i++) { %>
                    <tr>
                        <td><%= Guru.gurus['0'].daten[Guru.gurus['0'].details.currentGuruYear][Guru.gurus['0'].details.MRQ].infoTable[i].cusip %></td>
                        <td><%= Guru.gurus['0'].daten[Guru.gurus['0'].details.currentGuruYear][Guru.gurus['0'].details.MRQ].infoTable[i].name %></td>
                        <td><%= Guru.gurus['0'].daten[Guru.gurus['0'].details.currentGuruYear][Guru.gurus['0'].details.MRQ].infoTable[i].sector %></td>
                        <td><%= (Guru.gurus['0'].daten[Guru.gurus['0'].details.currentGuruYear][Guru.gurus['0'].details.MRQ].infoTable[i].value / Guru.gurus['0'].daten[Guru.gurus['0'].details.currentGuruYear][Guru.gurus['0'].details.MRQ].totalValue).toFixed(2) %></td>
                        <td><%= Guru.gurus['0'].daten[Guru.gurus['0'].details.currentGuruYear][Guru.gurus['0'].details.MRQ].infoTable[i].shares %></td>
                        <td><%= Guru.gurus['0'].daten[Guru.gurus['0'].details.currentGuruYear][Guru.gurus['0'].details.MRQ].infoTable[i].shares %></td>
                        <td></td>
                    </tr>
                    <% } %>
            </tbody>
    
        </table>
    
    </div>

</div>
